<?xml version="1.0" encoding="UTF-8"?>
<procsettings projectname = "example" version = "1.0">

<importfiles rootname = "C:\Users\slocumr.ONID\gitHub\SimUAS\data\demobeaver">
	<images     foldername = "output\images" imagesToUse = "1101111111111111111111111111111111111111111111111"/>
	<sensor     filename = "output\Sensor_photoscan.xml" lock = "1"/>
	<trajectory filename = "output\trajectory_norpy.xml"/>
	<controldata filename = "output\pixelcontrol.xml"/>
</importfiles>

<photoscan>
	<referencesettings>
		<measurementaccuracy camerapos = "10" camerarot = "2" marker = "0.005" scalebar = "0.001"/>
		<imageaccuracy marker = "0.1" tiepoint = "2" />
		<miscellaneous groundalt = ""/>
	</referencesettings>
	
	<optimize execute = "1" fits = "1111111111111"/>
	
	<aligngeneral accuracy="high" genericpre ="1" referencepre ="1"/>
	<alignadvanced keypointlim = "40000" tiepointlim = "4000" maskconstrain="0" adaptivecam = "1"/>
	<dense quality="low" depthfilt = "mild"/>
	<meshgeneral surftype="arbitrary" sourcedata = "dense" facecount = "medium" />
	<meshadvanced inteprolation="enabled" />
	<texturegeneral mappingmode = "generic" blendingmode = "mosaic" texsize = "4096" texcount = "1"/>
	<textureadvanced colorcorrection="0" holefill="1"/>
	<tiledmodel source="dense" pixelsize = "0.05" tilesize = "256"/>
	
	<dem>
		<projection type = "geographic" coordinates="local"/>
		<params sourcedata = "dense" interpolation = "enabled"/>
		<pixelsize resolution="0.05"/>
		<region minx="-50" maxx="50" miny="-50" maxy="50" />
	</dem>
	
	<ortho>
		<projection type = "geographic" coordinates = "local" />
		<params surface = "mesh" blending = "mosaic" colorcorr = "0" holefill = "0"/>
		<pixelsize x="0.05" y="0.05"/>
		<region minx="-50" maxx="50" miny="-50" maxy="50" resolution = "0.05" />
	</ortho>
</photoscan>

